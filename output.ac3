
power:
    BeginFunc 28
    t0 = 1
    result = t0
    t1 = 0
    i = t1
L1:
    if i < exp goto L2
    goto L3
L2:
    t2 = result * base
    result = t2
    t3 = 1
    t4 = i + t3
    i = t4
    goto L1
L3:
    Return result
    EndFunc

main:
    BeginFunc 24
    t5 = 2
    base = t5
    t6 = 8
    exponent = t6
    PushParam exponent
    PushParam base
    t7 = LCall power
    PopParams 8
    answer = t7
    EndFunc
